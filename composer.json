{
    "name": "dynamic/silverstripe-geocoder",
    "description": "SilverStripe wrapper for Geocoder",
    "authors": [
        {
            "name": "Dynamic, Inc",
            "email": "dev@dy.ag"
        }
    ],
    "keywords": [
        "silverstripe", "geocoder", "geocoding"
    ],
    "type": "silverstripe-module",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-cms": "^1.0",
        "willdurand/geocoder": "v3.3.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "config": {
        "process-timeout": 600
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\SilverStripeGeocoder\\": "src/",
            "Dynamic\\SilverStripeGeocoder\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-name": "geocoder",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\SilverStripeGeocoder\\": "code/"
        }
    }
}
